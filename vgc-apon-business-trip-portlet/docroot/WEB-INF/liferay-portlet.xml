<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	
	<portlet>
		<portlet-name>business-trip-application</portlet-name>
		<icon>/icon.png</icon>
		<scheduler-entry>
			<scheduler-event-listener-class>com.business.trip.util.AutoCloseMessageListener</scheduler-event-listener-class>
			<trigger>
				<cron>
					 
					<!-- <cron-trigger-value>0 0/1 * * * ?</cron-trigger-value> -->	
					
					<cron-trigger-value>0 10 1 * * ?</cron-trigger-value>		
				</cron>
			</trigger>
		</scheduler-entry>
		<asset-renderer-factory>com.business.trip.asset.BusinessTripApplicationAssetRenderFactory</asset-renderer-factory>
		<workflow-handler>com.business.trip.workflow.BusinessTripApplicationWorkflowHandler</workflow-handler>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			business-trip-application-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>business-trip-reimbursement</portlet-name>
		<icon>/icon.png</icon>
		<asset-renderer-factory>com.business.trip.asset.BusinessTripReimbursementAssetRenderFactory</asset-renderer-factory>
		<workflow-handler>com.business.trip.workflow.BusinessTripReimbursementWorkflowHandler</workflow-handler>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			business-trip-reimbursement-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>business-trip-exchange-rate</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			business-trip-exchange-rate-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>
			sap-pending-business-trip-application
		</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			sap-pending-business-trip-application-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>
			sap-pending-business-trip-reimbursement
		</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			sap-pending-business-trip-reimbursement-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>travel-expense-allowance</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			travel-expense-allowance-portlet
		</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>