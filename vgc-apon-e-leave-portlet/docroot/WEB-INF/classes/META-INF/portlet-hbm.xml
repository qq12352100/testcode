<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.e.leave.model.ELeave" />
	<import class="com.e.leave.model.ELeaveInfo" />
	<class name="com.e.leave.model.impl.ELeaveImpl" table="vgcapon_ELeave">
		<cache usage="read-write" />
		<id name="eLeaveId" type="long" access="com.liferay.portal.dao.orm.hibernate.CamelCasePropertyAccessor">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="ticketNo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="staffName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="staffCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="company" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="division" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="department" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="costCenter" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="officePhone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mobilePhone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="positionType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="isApplicantAgent" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="isApproverAgent" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="processType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="subProcessType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="submittedDate" type="org.hibernate.type.TimestampType" />
		<property name="comments" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="statusByUserId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="statusByUserName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="statusDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.e.leave.model.impl.ELeaveInfoImpl" table="vgcapon_ELeaveInfo">
		<cache usage="read-write" />
		<id name="eLeaveInfoId" type="long" access="com.liferay.portal.dao.orm.hibernate.CamelCasePropertyAccessor">
			<generator class="assigned" />
		</id>
		<property name="eLeaveId" access="com.liferay.portal.dao.orm.hibernate.CamelCasePropertyAccessor" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="startDate" type="org.hibernate.type.TimestampType" />
		<property name="endDate" type="org.hibernate.type.TimestampType" />
		<property name="startTime" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="endTime" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="typeOfLeave" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="typeOfLeaveId" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="absenceDays" type="com.liferay.portal.dao.orm.hibernate.StringType" />
