<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>vgc-apon-e-onboarding</portlet-name>
		<display-name>E Onboarding</display-name>
		<portlet-class>com.e.onboarding.portlet.eOnboarding.EOnBoardingPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/eOnBoarding/view.jsp</value>
		</init-param>
		<init-param>
			<name>add-process-action-success-action</name>
			<value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>E-Onboarding</title>
			<short-title>E-Onboarding</short-title>
			<keywords>E-Onboarding</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>portletSetupShowBorders</name>
				<value>false</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>distribution</portlet-name>
		<display-name>Distribution</display-name>
		<portlet-class>com.e.onboarding.portlet.distribution.DistributionPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/distribution/view.jsp</value>
		</init-param>
		<init-param>
			<name>add-process-action-success-action</name>
			<value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>HR Distribution List</title>
			<short-title>HR Distribution List</short-title>
			<keywords>HR Distribution List</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>portletSetupShowBorders</name>
				<value>false</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>itp-distribution</portlet-name>
		<display-name>ITP Distribution</display-name>
		<portlet-class>com.e.onboarding.portlet.distribution.ITPDistributionPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/distribution/view.jsp</value>
		</init-param>
		<init-param>
			<name>add-process-action-success-action</name>
			<value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>ITP Distribution List</title>
			<short-title>ITP Distribution List</short-title>
			<keywords>ITP Distribution List</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>portletSetupShowBorders</name>
				<value>false</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>