<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.seatoffice">
	<author>Alice.zou</author>
	<namespace>vgcapon</namespace>

	<entity name="SeatOffice" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="seatId" type="long" primary="true" />


		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="companyId" type="long" />

		<!-- seat -->

		<column name="ticketNo" type="String"/>
		<column name="staffName" type="String" />
		<column name="staffCode" type="long" />
		<column name="companyName" type="String" />
		<column name="division"  type="String"/>
		<column name="department"  type="String"/>
		<column name="costCenter"  type="String"/>
		<column name="officePhone"  type="String"/>
		<column name="mobilePhone"  type="String"/>
		<column name="officeSite"  type="String"/>
		<column name="currentCategory" type="long" />
        <column name="currentSeatNo"  type="String"/>
        <column name="movingDate"  type="Date"/>
        <column name="expectedSeatNo"  type="String"/>
        <column name="reasonForChange"  type="String"/>
        <column name="officeKey"  type="boolean"/>
        <column name="plant"  type="boolean"/>
        <column name="telephoneAndExtNo"  type="boolean"/>
        <column name="namePlate"  type="boolean"/>
        <column name="cabinet"  type="boolean"/>
        <column name="isOnBehalfOf" type="boolean"></column>
        <column name="nowCategory" type="long"></column>  
        <column name="newSeatNo" type="String"></column>  
        <column name="workspaceId" type="long"></column>  
        <column name="seatStatus" type="String"></column>
        <column name="sg" type="String"></column> 
        <column name="leadingMark" type="String"></column>
        <column name="positionFlag" type="String"></column>
        <column name="isApplicantAgent" type="boolean"></column>
        <column name="isApproverAgent" type="boolean"></column> 
		<!-- Candidate Information -->

		<column name="candidateName"  type="String"/>
		<column name="desiredStartingDate"  type="Date"/>
		
		<column name="comments"  type="String"/>

		<!-- Common Parts -->
        <column name="contractType"  type="String"/>
		<column name="processType" type="String" />
		<column name="subProcessType" type="String" />

		<!-- Workflow -->

		<column name="status" type="int" />
		<column name="statusByUserId" type="long" />
		<column name="statusByUserName" type="String" />
		<column name="statusDate" type="Date" />

		<!-- Order -->

		<order by="asc">
			<order-column name="seatId" />
		</order>

		<finder name="S_U" return-type="Collection">
			<finder-column name="status" />
			<finder-column name="userId" />
		</finder>
	</entity>
</service-builder>